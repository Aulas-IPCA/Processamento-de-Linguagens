digraph {
	start [label="" shape=none]
	start -> q1 [label=""]
	q1 [label=q1 shape=circle]
	q2 [label=q2 shape=circle]
	q3 [label=q3 shape=circle]
	q4 [label=q4 shape=circle]
	q5 [label=q5 shape=circle]
	q6 [label=q6 shape=circle]
	q1 -> q2 [label=a]
	q1 -> q3 [label=b]
	q2 -> q1 [label=a]
	q2 -> q4 [label="€"]
	q3 -> q1 [label=b]
	q3 -> q4 [label="€"]
	q4 -> q2 [label=a]
	q4 -> q3 [label=b]
	q5 -> q5 [label=a]
	q5 -> q6 [label=b]
	q6 -> q5 [label=a]
	q6 -> q6 [label=b]
}
